{
	"minify": true,
	"jsc": {
		"minify": {
			"compress": true,
			"mangle": true
		},
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"numericSeparator": false,
			"classPrivateProperty": false,
			"privateMethod": false,
			"classProperty": false,
			"functionBind": false,
			"decorators": false,
			"decoratorsBeforeExport": false
		},
		"transform": {
			"react": {
				"runtime": "automatic"
			}
		}
	},
	"module": {
		"type": "commonjs"
 }
}
